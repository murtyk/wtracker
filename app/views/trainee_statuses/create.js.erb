$('#new_trainee_status').remove(); // remove form
<% if @trainee_status.errors.any? %>
  $('#<%=new_link_id("trainee_status", "trainee_#{@trainee_status.trainee.id}_")%>').hide().after('<%= j render("jsform", trainee_status: @trainee_status) %>');
<% else %>
  $('#<%=new_link_id("trainee_status", "trainee_#{@trainee_status.trainee.id}_")%>').show();
  $('#trainee_<%=@trainee.id%>_trainee_statuses').html('<%= j render @trainee.trainee_statuses %>');
<% end %>
