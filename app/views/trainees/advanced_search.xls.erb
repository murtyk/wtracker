<table border="1">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <% if current_grant.trainee_applications? %>
      <th>Applied On</th>
      <th>Status</th>
    <% end %>
    <th>Email</th>
    <th>Mobile No</th>
    <th>County</th>
    <% if current_grant.trainee_applications? %>
      <th>Last Job Title</th>
      <th>Last Salary</th>
      <th>Industry</th>
    <% end %>
    <th>Funding Source</th>

    <th>Education</th>
    <th>Veteran</th>
    <% if current_grant.trainee_applications? %>
      <th>Skills</th>
      <th>Navigator</th>
    <% end %>
    <th>Classes</th>
    <th>Assessment</th>
  </tr>
<% @trainees.each do |t| %>
  <tr>
    <td><%= t.first %></td>
    <td><%= t.last %></td>
    <% if current_grant.trainee_applications? %>
      <td><%= t.applicant.applied_on %></td>
      <td><%= t.grant_trainee_status.name %></td>
    <% end %>
    <td><%= t.email %></td>
    <td><%= t.mobile_no %></td>
    <td><%= t.home_address && t.home_address.county %></td>
    <% if current_grant.trainee_applications? %>
      <td><%= t.applicant.last_job_title %></td>
      <td><%= t.applicant.last_wages %></td>
      <td><%= t.applicant.sector_name %></td>
    <% end %>
    <td><%= t.funding_source && t.funding_source.name %></td>
    <td><%= t.education %></td>
    <td><%= t.veteran ? 'Yes' : 'No' %></td>
    <% if current_grant.trainee_applications? %>
      <td><%= t.job_search_profile.skills %></td>
      <td><%= t.applicant.navigator_name %></td>
    <% end %>
    <td><%= t.klasses.map{ |k| k.name }.join('<br>').html_safe %></td>
    <td><%= t.assessments.map{ |a| a.name }.join('<br>').html_safe %></td>
  </tr>
<% end %>
</table>
