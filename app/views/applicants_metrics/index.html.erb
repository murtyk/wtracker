<h1>DASHBOARD</h1>

<table class="table table-condensed table-bordered table-striped table-hover">
  <tr>
    <% @data.header.each do |h| %>
      <th><%= h %></th>
    <% end %>
  </tr>
  <% @data.rows.each do |row| %>
    <tr>
      <% row.each do |d| %>
        <td><%= d %></td>
      <% end %>
    </tr>
  <% end %>
</table>

<% @data.fs_data.each do |fs_data| %>
  <hr>
  <table class="table table-condensed table-bordered table-striped table-hover">
    <tr>
      <% fs_data.header.each do |h| %>
        <th><%= h %></th>
      <% end %>
    </tr>
    <% fs_data.rows.each do |row| %>
      <tr>
        <% row.each do |d| %>
          <td><%= d %></td>
        <% end %>
      </tr>
    <% end %>
  </table>

<% end %>