<div class = 'row-fluid' id= 'import-div' style='background-color: #fff;padding-top: 10px;margin: 5px auto;'>
  <div class = 'span7'>
    <%= select_tag "county_ids", options_for_select(analyzer.counties_for_selection), { multiple: true } %>
    <a href="/job_searches/sort_and_filter/?id=<%= job_search_id%>" id = 'filter-button' class='btn btn-small btn-flat btn-primary' style='margin-left:20px'>
      <i class="icon-filter icon-white"></i> Filter
    </a>

    <a href="/job_searches/sort_and_filter/?id=<%= job_search_id%>" id = 'clear-filter-button' class='btn btn-small btn-flat btn-primary' style='margin-left:20px'>
      All
    </a>
  </div>

  <div class = 'span5'>
    Sectors <%= select_tag "sector_ids", options_from_collection_for_select(Sector.all, "id", "name", current_user.last_sectors_selected), {multiple: true}%>

    <button type="button" id = 'import-from-google' class="btn btn-flat btn-small btn-primary" name='search_google_places'>
      Import All
    </button>
  </div>
</div>

<div class='row-fluid' id = 'legend' style='min-height: 30px; text-align: center;vertical-align: middle;background-color: #eee'>
  <div class = 'span4' style='min-height: 30px;padding-top:5px'><strong>Probability of match</strong></div>
  <div class = 'span2' style='margin-left: 0;padding-top:5px; background-color: rgb(223, 240, 216)'><strong>>95%</strong></div>
  <div class = 'span2' style='margin-left: 0;padding-top:5px; background-color: rgb(217, 237, 247)'><strong>>75%</strong></div>
  <div class = 'span2' style='margin-left: 0;padding-top:5px; background-color: rgb(252, 248, 227)'><strong>>65%</strong></div>
  <div class = 'span2' style='margin-left: 0;padding-top:5px; background-color: rgb(242, 222, 222)'><strong>below 65% or not found</strong></div>
</div>
