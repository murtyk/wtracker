$('#edit_trainee_note_<%=@trainee_note.id%>').remove();
<% if @trainee_note.errors.any? %>
  $('#<%=div_id_for(@trainee_note)%>').hide().after('<%= j render("jsform", trainee_note: @trainee_note) %>');
<% else %>
  $('#<%=div_id_for(@trainee_note)%>').replaceWith('<%= j render(@trainee_note) %>');
  $('#<%=div_id_for(@trainee_note)%>').show();
<% end %>