<% if @special_service.errors.any? %>
  $('#error_text').text('<%= @special_service.errors.first[1] %>');
<% else %>
  $('#error_text').text('');
  $('#new_special_service').remove();
  $('#special_services').html('<%= j render @special_services %>');
  $('#new_special_service_link').show();
<% end %>
