<table id="data-table" class="table table-condensed table-bordered table-striped table-hover">
  <tr style="display:none;">
    <th><%= report.title%></th>
    <%= ('<th></th>' * 7).html_safe %>
  </tr>

  <tr>
    <th>Name</th>
    <th>Class</th>
    <th>Funding Source</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>College</th>
    <th>Status</th>
    <th>Notes</th>
  </tr>

  <% report.trainees_not_placed.each do |kt| %>
  	<% if kt.klass %>
    <tr>
      <td><%= link_to(kt.name, kt.trainee)%></td>
      <td><%= link_to kt.klass_name, kt.klass%></td>
      <td><%= kt.trainee.funding_source_name %></td>
      <td><%= kt.start_date%></td>
      <td><%= kt.end_date%></td>
      <td><%= kt.college_name_location%></td>
      <td><%= kt.status %></td>
      <td><%= kt.notes %></td>
    </tr>
    <% end %>
  <% end %>
</table>

