<table id="data-table" class="table table-condensed table-bordered table-striped table-hover">
  <tr style="display:none;">
    <th><%= report.title%></th>
    <%= ('<th></th>' * 13).html_safe %>
  </tr>
  <thead>
  <tr>
    <th>Name</th>
    <th>Student ID</th>
    <th>Class</th>
    <th>Funding Source</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>College</th>
    <th>Notes</th>
    <th>Status</th>
    <th>Employer</th>
    <th>Location</th>
    <th>Title</th>
    <th>Start Date</th>
    <th>Pay</th>
  </tr>
  </thead>
  <% report.trainee_statuses.each do |trainee_status| %>
    <tr>
      <% trainee_status.each do |d| %>
      <td><%= d %></td>
      <% end %>
    </tr>
  <% end %>
</table>
