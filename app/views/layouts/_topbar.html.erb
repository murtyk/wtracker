<% if user_signed_in? && current_user%>
  <%= render 'layouts/clienttopbar' %>
<% elsif admin_signed_in? %>
  <%= render 'layouts/admintopbar' %>
<% elsif trainee_signed_in? %>
  <%= render 'layouts/trainee_portal_topbar' %>
<% elsif controller_name == 'shared_job_statuses' %>
  <%= render 'layouts/traineetopbar' %>
<% elsif controller_name == 'job_search_profiles' %>
  <%= render 'layouts/auto_job_leads' %>
<% elsif controller_name == 'applicants' %>
  <%= render 'layouts/trainee_portal_topbar' %>
<% elsif request.path == '/trainees/sign_in' || request.path == '/trainees/password/new' %>
  <%= render 'layouts/trainee_portal_topbar' %>
<% else %>
  <%= render 'layouts/hometopbar' %>
<% end %>