$('#new_klass_title').remove();
<% if @klass_title.errors.any? %>
  $('#new_klass_title_link').hide().after('<%= j render("jsform", klass_title: @klass_title) %>');
<% else %>
  $('#titles').html('<%= j render("list", klass_titles: @klass_title.klass.klass_titles) %>');
  $('#new_klass_title_link').show();
  refresh_job_counts([<%=@klass_title.id%>]);
<% end %>